{
  "name": "address-book",
  "version": "1.0.0",
  "description": "To store addresses of contacts and places",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist *.tsbuildinfo",
    "pretest": "npm run clean && npm run build",
    "migrate": "node ./dist/migrate",
    "start": "npm run build && nodemon -r source-map-support/register .",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prepare": "npm run build",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.5.0",
    "@loopback/context": "^1.21.1",
    "@loopback/core": "^1.9.0",
    "@loopback/openapi-v3": "^1.9.1",
    "@loopback/repository": "^1.10.1",
    "@loopback/rest": "^1.16.6",
    "@loopback/rest-explorer": "^1.3.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cryptr": "^4.0.0",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/nodemailer": "^6.2.1",
    "bcrypt": "^3.0.6",
    "cryptr": "^4.0.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mongodb": "^4.2.0",
    "nodemailer": "^6.3.0"
  },
  "devDependencies": {
    "@loopback/eslint-config": "^2.0.0",
    "@loopback/testlab": "^1.7.2",
    "@types/node": "^10.14.14",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.13",
    "typescript": "~3.5.3"
  }
}
